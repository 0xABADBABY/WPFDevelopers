										<TransformGroup>
											<ScaleTransform />
											<SkewTransform />
											<RotateTransform />
											<TranslateTransform />
										</TransformGroup>
									</Grid.RenderTransform>
									<Ellipse Margin="0,50,0,50">
										<Ellipse.Fill>
